extend layout 

block content
  div.pageHeader
    h3 Materials
    a(href='/material/create') Create New Material
  hr
  div.materials
    each mat in mats
      div.material
        a(href=mat.url) 
          img(src=mat.pic alt=mat.name style='width:50px; height:50px')
        
    else 
      | No Materials Found

  
